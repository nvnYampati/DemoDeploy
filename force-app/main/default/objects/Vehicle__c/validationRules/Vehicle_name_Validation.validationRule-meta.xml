<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Vehicle_name_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>LEN( Name )  &lt;&gt; 10
 || 
REGEX( Name , &quot;[A-Z]{2}-[0-9]{2}-[A-Z]{2}-[0-9]{4}&quot;)</errorConditionFormula>
    <errorMessage>Give a proper vehicle number</errorMessage>
</ValidationRule>
